#include <stdio.h>
#define SIZE 5

int q[SIZE], front = -1, rear = -1;

void enq(int v){
    if(rear==SIZE-1) printf("Overflow!\n");
    else{
        if(front==-1) front=0;
        q[++rear]=v;
        printf("%d enqueued.\n",v);
    }
}

void deq(){
    if(front==-1 || front>rear) printf("Underflow!\n");
    else printf("%d dequeued.\n",q[front++]);
}

void disp(){
    if(front==-1 || front>rear) printf("Queue empty.\n");
    else{
        for(int i=front;i<=rear;i++) printf("%d ",q[i]);
        printf("\n");
    }
}

int main(){
    int ch,v;
    do{
        printf("\n1.Enq 2.Deq 3.Disp 4.Exit\nChoice: ");
        scanf("%d",&ch);
        switch(ch){
            case 1: printf("Value: "); scanf("%d",&v); enq(v); break;
            case 2: deq(); break;
            case 3: disp(); break;
        }
    }while(ch!=4);
}
